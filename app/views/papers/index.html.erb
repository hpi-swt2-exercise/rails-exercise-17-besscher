<h1>all the papers are listed here!</h1>

<%= link_to 'Add paper', new_paper_path %>


<table>
  <tr>
    <th>Title</th>
    <th>Venue</th>
    <th>Year</th>
    <th>Show</th>
  </tr>

  <% @papers.each do |paper| %>
      <tr>
        <td><%= paper.title %></td>
        <td><%= paper.venue %></td>
        <td><%= paper.year %></td>
        <td><%= link_to 'Show', paper %></td>
      </tr>
  <% end %>
</table>